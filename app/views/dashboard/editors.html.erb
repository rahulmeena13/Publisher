<%= render 'dashboard_header' %>
<span class="label label-inverse">Listing Editors</span>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Username</th>
      <th>Email</th>
      <th>Edit</th>
      <th>Delete</th>
    </tr>
  </thead>

  <tbody>
    <% @editors.each do |editor| %>
      <tr>
        <td><%= editor.username %></td>
        <td><%= editor.email %></td>
        <td><%= link_to 'Edit', edit_editor_path(editor), :class =>'btn btn-small'  %></td>
        <td><%= link_to 'Delete', delete_editor_path(editor), method: :delete, data: { confirm: 'Are you sure?' }, :class =>'btn btn-small'  %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New Editor', new_editor_path, :class =>'btn btn-small' %>
<%= render 'dashboard_footer' %>
