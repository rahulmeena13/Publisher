<%= render 'dashboard_header' %>
<h1>Listing Unpublished Articles</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>By</th>
      <th>Show</th>
    </tr>
  </thead>

  <tbody>
    <% @articles.each do |article| %>
      <tr>
        <td><%= article.title %></td>
        <td><%= article.user.username %></td>
        <td><%= link_to 'Show', article %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= render 'dashboard_footer' %>
